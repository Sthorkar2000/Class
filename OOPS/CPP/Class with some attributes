#Program to print class with some attributes.

PROGRAM

#include <iostream>
using namespace std;

class car{
    public:
    string brand;
    string model;
    int year;
};


int main()
{
    car carObj1;
    carObj1.brand ="BMW";
    carObj1.model ="X5";
    carObj1.year =1995;
    
    car carObj2;
    carObj2.brand ="Ford";
    carObj2.model ="Mustang";
    carObj2.year =1969;
    
    //Print
    cout<<carObj1.brand<<" "<<carObj1.model<<" "<<carObj1.year <<"\n";
    cout<<carObj2.brand<<" "<<carObj2.model<<" "<<carObj2.year <<"\n";
    return 0;
}
